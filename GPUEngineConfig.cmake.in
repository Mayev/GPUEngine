set(_supported_components @gpuengine_components@)

foreach(_comp ${GPUEngine_FIND_COMPONENTS})
  if (NOT ";${_supported_components};" MATCHES ${_comp})
    set(CGPUEngine_FOUND False)
    set(GPUEngine_NOTFOUND_MESSAGE "Specified unsupported component: ${_comp}")
  else()
    include("${CMAKE_CURRENT_LIST_DIR}/../${_comp}/${_comp}Config.cmake")
  endif()
endforeach()
